{
  "name": "remotion-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "remotion": "remotion studio",
    "render": "remotion render",
    "deploy": "node deploy.mjs",
    "download-font": "node scripts/download-google-font.mjs",
    "download-fonts": "node scripts/download-top-fonts.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:run": "vitest run",
    "lint:fix": "eslint src --fix; tsc --noEmit",
    "fix": "prettier --write src && eslint src --fix --fix-type problem,suggestion,layout; tsc --noEmit",
    "fix:all": "prettier --write . && eslint . --fix --fix-type problem,suggestion,layout",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit 2>&1 || echo 'Typescript check failed'",
    "lint": "eslint src 2>&1 || echo 'Lint check failed'"
  },
